<header>
  <h1>TinyApp</h1>
  <% if(user_id) { %>
  <div>
    <p>
      <%= email %>
      <form action="/logout" method=POST>
        <input type="submit" value="Logout">
      </form>
    </p>
  </div>
  <% } else { %>
    <a href="/login">Login<a> <a href="/register">Register<a>
  <% } %>
</header>