<!DOCTYPE html>
  <html lang="en">
    <head>
      <title>TinyApp</title>
    </head>
    <body>
      <% include partials/_header %>
      <a href="/urls/new">Create a New Short Link</a>
      <ul>
        <% for(url in urls) {%>
          <li><a href=<%= urls[url].longURL %>><%= url %><a>:<%= urls[url].longURL %></li>
          <a href='/urls/<%= url %>'>Edit<a>
          <form method="POST" action="/urls/<%= url %>/delete">
            <input type="submit" value="Delete">
          </form>
        <% } %>
      </ul>
    </body>
</html>